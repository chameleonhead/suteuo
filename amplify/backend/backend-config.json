{
  "hosting": {
    "amplifyhosting": {
      "service": "amplifyhosting",
      "type": "cicd"
    }
  },
  "auth": {
    "suteuo": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false
    }
  },
  "storage": {
    "suteuomessaging": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "suteuousers": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "suteuousers": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "suteuo",
          "attributes": [
            "UserPoolId"
          ]
        },
        {
          "category": "storage",
          "resourceName": "suteuousers",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "suteuomessaging": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "suteuomessaging",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "api": {
    "suteuo": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "suteuousers",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "suteuomessaging",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}